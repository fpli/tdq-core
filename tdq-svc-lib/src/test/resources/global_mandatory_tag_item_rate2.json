{
  "id": "1",
  "rules": [
    {
      "name": "rule_1",
      "type": "realtime.rheos.profiler",
      "config": {
        "window": "2min"
      },
      "profilers": [
        {
          "metric-name": "global_mandatory_tag_item_rate2",
          "dimensions": [
            "domain",
            "site"
          ],
          "filter": "(domain in ('ASQ') or domain in ('VI','WTCH','XO')) and IS_BBWOA_PAGE_WITH_ITM(page_id) and clientData.remoteIP not like '10.%' and site in('1','3')",
          "expr": "itm_valid_cnt / itm_cnt",
          "transformations": [
            {
              "alias": "page_id",
              "expr": "CAST( SOJ_NVL('p') AS INTEGER)"
            },
            {
              "alias": "domain",
              "expr": "soj_page_family(page_id)"
            },
            {
              "alias": "site",
              "expr": "SOJ_NVL('t')"
            },
            {
              "alias": "itm_cnt",
              "expr": "count(1)"
            },
            {
              "alias": "itm_valid_cnt",
              "expr": "Sum(case when CAST( SOJ_NVL('itm|itmid|itm_id|itmlist|litm') AS LONG) is not null then 1 else 0 end)"
            }
          ]
        }
      ]
    }
  ]
}